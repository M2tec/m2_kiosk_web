[Unit]
Description=m2kiosk.service - A Flask application run with Gunicorn.
After=network.target
 
[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/m2_kiosk_web/
ExecStart=/usr/bin/gunicorn --workers 3 --bind unix:/var/www/m2_kiosk_web/m2paypad.sock wsgi:app
 
[Install]
WantedBy=multi-user.target
